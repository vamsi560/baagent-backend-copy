{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "250mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "buildCommand": "pip install -r requirements-vercel-lightweight.txt",
  "env": {
    "GEMINI_EMBEDDING_DIMENSION": "768"
  }
}
